{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    // тут хост указывается как localhost и порт внешний, так же контейнер бд должен быть запущен.
    "ScanPersonDb": "Host=localhost;Database=ScanPerson;User ID=postgres;Password=postgres;Port=5433;Pooling=true;"
  },
  "JwtOptions": {
    "SecretKey": "mySuperSecretKeyWithSizeGreatherThan256Bit",
    "Issuer": "Issuer",
    "Audience": "Audience",
    "ExpireHours": 8
  },
  "Graylog": {
    "Host": "graylog",
    "Port": 12201
  }
}
