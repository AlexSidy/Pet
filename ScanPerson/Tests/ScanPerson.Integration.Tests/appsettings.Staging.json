{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    // тут хост указывается как localhost и порт внешний, так же контейнер бд должен быть запущен.
    "ScanPersonDb": "Host=localhost;Database=ScanPerson;User ID=postgres;Password=postgres;Port=5433;Pooling=true;",
    "RedisConnection": "redis:6379"
  },
  "JwtOptions": {
    "SecretKey": "myTestingSuperSecretKeyWithSizeGreatherThan256Bit",
    "Issuer": "Issuer",
    "Audience": "Audience",
    "ExpireHours": 8
  },
  "Graylog": {
    "Host": "graylog",
    "Port": 12201
  },
  "ServiceOptions": {
    "GeoServiceOptions": {
      "BaseUrl": "https://htmlweb.ru/geo/api.php"
    },
    "BrowserBotServiceOptions": {
      "ServiceHostOptions": {
        "Host": "http://browserbot.api/",
        "Port": 8070,
        "ApiVersion": "api",
        "ControllerName": "BrowserBot"
      }
    },
    // Должно быть точное имя сервиса из бизнес логики.
    "UnUsingServices": [
      "TestUnusedService"
    ]
  }
}
